@media print {
    body {
        background:#ffffff;
        font:normal 12pt times, "times new roman", georgia, serif;
        color:#000;
        margin:0 0 20px;
    }
    /* turn everything off */
    *.wrapHeader, *.wrapNav, *.wrapMedia, *.contentSecondary,  *.wrapFooter {
        display:none;
    }
    /* turn main content back on */
        *.contentPrimary, *.contentPrimary .gutter {
        width:100%;
        margin:0;
        padding:0;
        float:none;
    }
    *.wrapContentOuter {
        margin:0;
        padding:0;
        position:static;
        float:none;
        width:100%;
        background:#fff;
    }
    /* printer-friendly font family */
        *.contentPrimary * {
        font-family:times, "times new roman", georgia, serif;
    }
}

