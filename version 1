@font-face {
font-family:'fontello';
src:url('file/fonts/fontello.eot?70547909');
src:url('file/fonts/fontello.eot?70547909#iefix') format('embedded-opentype'),
    url('file/fonts/fontello.woff?70547909') format('woff'),
    url('file/fonts/fontello.ttf?70547909') format('truetype'),
    url('file/fonts/fontello.svg?70547909#fontello') format('svg');
font-weight:normal;
font-style:normal;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
font-family:"fontello";
font-style:inherit;
font-weight:inherit;
speak:none;
display:inline-block;
text-decoration:inherit;
text-align:center;
font-variant:normal;
text-transform:none;
}

